{
  "name": "@kurasu-manta/api-schema",
  "version": "0.1.0",
  "description": "Shared API schema definitions and validation for Kurasu Manta",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.mjs",
      "require": "./build/index.js"
    },
    "./audio": {
      "types": "./build/audio.d.ts",
      "import": "./build/audio.mjs",
      "require": "./build/audio.js"
    }
  }
}
