{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": ["NODE_ENV"],
  "tasks": {
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["build"],
      "inputs": ["**/*.{ts,tsx,js,jsx}"]
    },
    "kurasu-manta-client#build:web": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "node_modules/.cache/metro/**"],
      "env": ["EXPO_PUBLIC_*"]
    },
    "kurasu-manta-client#build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "node_modules/.cache/metro/**"],
      "env": ["EXPO_PUBLIC_*"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**"]
    },
    "pre-deploy": {
      "dependsOn": ["build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
